    "categorical": [
      {"name": "Sex", "options": ["M", "F"]},
      {"name": "ChestPainType", "options": ["ATA", "NAP", "ASY", "TA"]},
      {"name": "RestingECG", "options": ["Normal", "ST", "LVH"]},
      {"name": "ExerciseAngina", "options": ["Y", "N"]},
      {"name": "ST_Slope", "options": ["Up", "Flat", "Down"]}

{
    "domain": "cardiology",
    "numerical": [
      {"name": "Age", "min": 0, "max": 120},
      {"name": "RestingBP", "min": 50, "max": 200},
      {"name": "Cholesterol", "min": 0, "max": 600},
      {"name": "FastingBS", "min": 0, "max": 1},
      {"name": "MaxHR", "min": 60, "max": 220},
      {"name": "Oldpeak", "min": -2, "max": 6}
